<%= form_for :article, url: article_path(@article), method: :patch do |f| %><p>
<%=f.label :title%><br>
<%=f.text_field :title%>
</p>

<p>
<%=f.label :description%><br>
<%=f.text_area :description%>
</p>

<p>
<%=f.label :image%><br>
<%=f.file_field :image%>
</p>

 <p>
    <%= f.submit %>
  </p>
<% end %>
